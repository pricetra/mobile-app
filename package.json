{
  "name": "pricetra-mobile",
  "main": "expo-router/entry",
  "version": "1.1.14",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "APP_VARIANT=development expo start",
    "start:tunnel": "APP_VARIANT=development expo start --tunnel",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "eas:new-device": "eas device:create",
    "eas:dev:build:android": "eas build -p android --profile development",
    "eas:dev:build:ios": "eas build -p ios --profile development",
    "eas:device:create": "eas device:create",
    "eas:publish:prod:ios": "eas build --platform ios --auto-submit --profile production",
    "eas:publish:prod:android": "eas build --platform android --auto-submit --profile production",
    "doctor": "expo-doctor",
    "install-check": "expo install --check",
    "bump-version": "node bumpVersion.js",
    "bump-version:minor": "node bumpVersion.js minor",
    "bump-version:major": "node bumpVersion.js major"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@cloudinary/url-gen": "^1.21.0",
    "@expo/vector-icons": "^14.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/checkbox": "^1.1.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/portal": "^1.1.0",
    "@rn-primitives/select": "^1.1.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/table": "^1.1.0",
    "@rn-primitives/tabs": "^1.2.0",
    "@rn-primitives/types": "^1.1.0",
    "addresser": "^1.1.20",
    "apollo-upload-client": "^18.0.1",
    "assert": "^2.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convert-units": "^2.3.4",
    "dayjs": "^1.11.13",
    "expo": "^53.0.0",
    "expo-apple-authentication": "~7.2.4",
    "expo-application": "~6.1.5",
    "expo-asset": "~11.1.7",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.8",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-network": "~7.1.5",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.10",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "formik": "^2.4.6",
    "graphql": "^16.10.0",
    "graphql-utils": "github:pricetra/graphql-utils",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.477.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-autocomplete-dropdown": "^4.4.0",
    "react-native-collapsible": "^1.6.2",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-currency-input": "^1.1.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-intersection-observer": "^0.2.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-super-grid": "^6.0.2",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/convert-units": "^2.3.11",
    "@types/lodash": "^4.17.16",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "expo": {
    "sdkVersion": "53.0.0",
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "overrides": {
    "lucide-react-native": {
      "react": "19.0.0"
    },
    "@react-native-community/datetimepicker": {
      "react": "19.0.0"
    }
  }
}
