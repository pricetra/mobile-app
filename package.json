{
  "name": "pricetra-mobile",
  "main": "expo-router/entry",
  "version": "1.0.5",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "APP_VARIANT=development expo start",
    "start:tunnel": "APP_VARIANT=development expo start --tunnel",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "gql-codegen": "graphql-codegen --config codegen.ts",
    "eas:new-device": "eas device:create",
    "eas:dev:build:android": "eas build -p android --profile development",
    "eas:dev:build:ios": "eas build -p ios --profile development",
    "eas:device:create": "eas device:create",
    "eas:publish:prod:ios": "eas build --platform ios --auto-submit --profile production",
    "doctor": "expo-doctor",
    "install-check": "expo install --check"
  },
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@cloudinary/url-gen": "^1.21.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/checkbox": "^1.1.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/portal": "^1.1.0",
    "@rn-primitives/select": "^1.1.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/table": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "addresser": "^1.1.20",
    "apollo-upload-client": "^18.0.1",
    "class-variance-authority": "^0.7.1",
    "cloudinary-react-native": "^1.0.1",
    "clsx": "^2.1.1",
    "convert-units": "^2.3.4",
    "dayjs": "^1.11.13",
    "expo": "^52.0.37",
    "expo-application": "~6.0.2",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-camera": "~16.0.17",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image": "~2.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.8",
    "expo-network": "~7.0.5",
    "expo-router": "^4.0.21",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "formik": "^2.4.6",
    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "lucide-react-native": "^0.477.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-autocomplete-dropdown": "^4.4.0",
    "react-native-collapsible": "^1.6.2",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-currency-input": "^1.1.1",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "expo-updates": "~0.27.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/client-preset": "^4.6.4",
    "@graphql-codegen/typescript": "^4.1.5",
    "@graphql-codegen/typescript-operations": "^4.5.1",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/convert-units": "^2.3.11",
    "@types/lodash": "^4.17.16",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true,
  "expo": {
    "sdkVersion": "52.0.0",
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "overrides": {
    "expo-modules-autolinking": "~2.0.0",
    "@expo/config-plugins": "~9.0.0",
    "@expo/prebuild-config": "^8.0.0",
    "@expo/metro-config": "~0.19.0"
  },
  "resolutions": {
    "expo-modules-autolinking": "~2.0.0",
    "@expo/config-plugins": "~9.0.0",
    "@expo/prebuild-config": "^8.0.0",
    "@expo/metro-config": "~0.19.0"
  }
}
